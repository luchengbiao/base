QCheckBox#game_chk_auto_taking_pieces{
	color:#f7d8c8;
	font-size:14px;
	border:none;
}

QCheckBox#game_chk_auto_taking_pieces::indicator{
	width:27px;
    height:29px;
}

QCheckBox#game_chk_auto_taking_pieces::indicator:unchecked{
	image:url(:go/res/images/go/auto_taking_uncheck.png);
}

QCheckBox#game_chk_auto_taking_pieces::indicator:checked{
	image:url(:go/res/images/go/auto_taking_check.png);
}

QWidget#game_wdt_online_msg_box{
