}

QPushButton#game_btn_close_online_msg_box{
	background-image:url(:common/res/images/common/online_msg_close.png);
	border:none;
}
QPushButton#game_btn_ok_online_msg_box
{
	background-image:url(:common/res/images/common/online_msg_ok_normal.png);
	border:none;
}
QPushButton#game_btn_ok_online_msg_box:hover
{
	background-image:url(:common/res/images/common/online_msg_ok_pressed.png);
}
QPushButton#game_btn_ok_online_msg_box:pressed
{
	background-image:url(:common/res/images/common/online_msg_ok_hover.png);
}
QPushButton#game_btn_ok_online_msg_box:disabled
{
	background-image:url(:common/res/images/common/online_msg_ok_disabled.png);
}

QListView#game_lv_common
{
	outline:0px;
}
QListView#game_lv_common::item
{
	color:rgba(0,0,0,0);
	padding-left:5px;
}
QListView#game_lv_common::item:selected
{
	background:rgba(102,102,102, 10%);
}
QListView#game_lv_common::item:hover
{
	background:rgba(102,102,102, 10%);
}
QListView#game_lv_common::item:disabled
{
	background:rgba(102,102,102, 10%);
	color:#CCCCCC;
}

QComboBox#game_cmb_common
{ 
	background-color:#F7F7F7; 
	border:none; 
	font-size:12px; 
	color:#666666; 
	padding-left:10px;
}
QComboBox#game_cmb_common::down-arrow
{
	image: url(:common/res/images/common/combo_arrow_down.png);
	width: 11px;
	height: 10px; 
}
QComboBox#game_cmb_common::down-arrow:on
{
	image: url(:common/res/images/common/combo_arrow_up.png);
}
QComboBox#game_cmb_common::down-arrow:disabled
{
	image: url(:common/res/images/common/combo_arrow_disabled.png);
}
QComboBox#game_cmb_common::drop-down
{
	border:none;
}	
QComboBox#game_cmb_common:disabled
{
	color:#cccccc;
	font-size:14px;
}
QComboBox QAbstractItemView#game_cmb_common
{
	color: rgb(102,102,102);
	background-color: #FFFFFF;
	selection-color: rgb(102,102,102);
	selection-background-color: #ECECEC;
	padding:0px,0px,0px,0px;
}
QComboBox QAbstractItemView#game_cmb_common::item
{
	min-height:30px;
}
QComboBox QAbstractItemView#game_cmb_common::item:selected
{
	min-height:30px;
}

